{
  "Type": "SubnetACLAssociation",
  "Description" : "Version 1.0.  Associates an ACL with a subnet",
  "Parameters" : {
    "ACL" : {
      "Description" : "ID of ACL",
      "Type" : "String",
      "Required": true
    },
    "Subnet" : {
      "Description" : "ID of Associated Subnet",
      "Type" : "String",
      "Required": true
    }
  },
  "Resources" : {
    "SubnetACLAssociation" : {
      "Type" : "AWS::EC2::SubnetNetworkAclAssociation",
      "Properties" : {
        "NetworkAclId" : {
          "Ref" : "ACL"
        },
        "SubnetId" : {
          "Ref" : "Subnet"
        }
      }
    }
  },
  "Connections":{
    "Substitutes":[
        {
            "Parameter": "ACL",
            "Type":"ACL"
        },
        {
            "Parameter": "Subnet",
            "Type":"Subnet"
        }
    ]
  }
}
