{
  "Type": "SubnetRoute",
  "Description" : "Version 1.0.  Adds a Route to an InternetGateway",
  "Parameters" : {
    "RouteTable" : {
      "Description" : "Host Route Table",
      "Type" : "String"
    },
    "Subnet" : {
      "Description" : "Subnet for Route",
      "Type" : "String"
    },
    "DestinationCidrBlock":{
      "Description": "Which traffic should be routed",
      "Type":"String"
    }
  },
  "Resources" : {
    "SubnetRoute" : {
      "Type" : "AWS::EC2::Route",
      "Properties" : {
        "DestinationCidrBlock" : {"Ref":"DestinationCidrBlock"},
        "RouteTableId" : {
          "Ref" : "RouteTable"
        },
        "SubnetId" : {
          "Ref" : "Subnet"
        }
      }
    }
  },
  "Connections":{
    "References":[],
    "Substitutes":[            
        {
            "Parameter": "RouteTable",
            "Type":"RouteTable"
        },
        {
            "Parameter": "Subnet",
            "Type":"Subnet"
        }
    ]
  }
}
